{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "src",
    "compilerOptions": {
        "deleteOutDir": true,
        "assets": [
            {
                "include": "../proto",
                "outDir": "dist/proto",
                "watchAssets": true
            },
            {
                "include": "../keys",
                "outDir": "dist/keys",
                "watchAssets": true
            }
        ]
    }
}
